#include<iostream>
using namespace std;
int main(){
int n;
cout<<"enter the number of lines:";
cin>>n;

for(int i=1; i<=n; i++){
    int a=1;
    for(int j=1; j<=n; j++){
        int d=a+64;
        char ch =(char)d;
        cout<<ch;
        a++;
    }
    cout<<endl;
}
return 0;
}


OUTPUT

enter the number of lines:8
ABCDEFGH
ABCDEFGH
ABCDEFGH
ABCDEFGH
ABCDEFGH
ABCDEFGH
ABCDEFGH
ABCDEFGH
